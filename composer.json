{
  "name": "treehouse/interview",
  "type": "project",
  "license": "proprietary",
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "bin-dir": "bin",
    "sort-packages": true,
    "preferred-install": {
      "*": "dist"
    }
  },
  "require": {
    "php": "^8.3",
    "ext-json": "*",
    "google/cloud-firestore": "^1.41",
    "google/cloud-pubsub": "^2.0",
    "symfony/console": "^7.0.3",
    "symfony/dotenv": "^7.0.3",
    "symfony/flex": "^2.4.4",
    "symfony/framework-bundle": "^7.0.3",
    "symfony/runtime": "^7.0.3",
    "symfony/yaml": "^7.0.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0.5",
    "symfony/var-dumper": "^7.0.3"
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-intl-grapheme": "*",
    "symfony/polyfill-intl-normalizer": "*",
    "symfony/polyfill-mbstring": "*",
    "symfony/polyfill-php81": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "extra": {
    "runtime": {
      "disable_dotenv": false,
      "use_putenv": true
    },
    "symfony": {
      "allow-contrib": false,
      "require": "^7.0"
    }
  }
}
